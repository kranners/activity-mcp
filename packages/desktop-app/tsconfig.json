{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "outDir": "./dist",
    "rootDir": "./src"
  },
  "exclude": [
    "eslint.config.ts",
    "jest.config.ts",
    "**/*.tsx",
    "vite.config.ts",
    ".api",
    "dist",
    "node_modules"
  ]
}
